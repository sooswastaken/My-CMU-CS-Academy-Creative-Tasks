def drawClouds(centerX,centerY):
    Oval(centerX,centerY,68,12,fill='white',opacity=11)
    
def groupOfClouds(centerX,centerY):
    drawClouds(centerX-12,centerY+6)  
    drawClouds(centerX+10,centerY-3)
    
