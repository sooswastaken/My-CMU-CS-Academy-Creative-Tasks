###Time counter
time = Label('0',322,22,size=27,fill=rgb(207,207,207),font='arial')
Label(': AM',363,22,size=27,fill=rgb(207,207,207),font='arial')

#Light growing on lighthouse
lightCircle = Circle(345,72,28,fill=rgb(255,215,0),opacity=30)
lightCircle2 = Circle(367,56,20,fill=rgb(255,215,0),opacity=30)

message = Label('It is passed 3:AM',200,200,fill=rgb(0,248,237),font='monospace',visible=False)


def onKeyPress(key):
    if(key == '0'):
        app.start=True
        app.oneoclock=False
        app.twooclock=False
        app.threeoclock=False
        
    if(key == '1'):
        app.start=False
        app.oneoclock=True
        app.twooclock=False
        app.threeoclock=False
        
    if(key == '2'):
        app.start=False
        app.oneoclock=False
        app.twooclock=True
        app.threeoclock=False
    
    if(key == '3'):
        app.start=False
        app.oneoclock=False
        app.twooclock=False
        app.threeoclock=True
        app.bill=True
       
    if(key == 'up'):
        app.time+=1
        
    elif(key == 'down'):
        app.time-=1
        
    ###### CUSTOM PROPERTIES                                                C U S T O M   P R O P E R T I E S
    ###Monsters
    if(app.time == 0):
        time.value=0
        lightCircle.radius=28
        lightCircle2.radius=20
        lightCircle.opacity=30
        lightCircle2.opacity=30
        
            
    elif(app.time == 1):
        time.value=1
        lightCircle.radius = 24
        lightCircle2.radius = 16
        lightCircle.opacity=25
        lightCircle2.opacity=25

        
    elif(app.time == 2):
        time.value=2
        lightCircle.radius = 22
        lightCircle2.radius = 14
        lightCircle.opacity = 20
        lightCircle2.opacity = 20

        
    elif(app.time == 3):
        time.value=3
        lightCircle.radius = 20
        lightCircle2.radius = 12
        lightCircle.opacity = 15
        lightCircle2.opacity = 15
        
    
    elif(app.time == 4):
        time.value=4
        
        lightCircle.opacity = 10
        lightCircle2.opacity = 10
        lightCircle.radius = 18
        lightCircle2.radius = 10
        
    elif(app.time > 4):
        time.value= app.time
        if(app.threeoclock == False):
            billLeftLeg.toFront()
            billLeftLeg2.toFront()
            billRightLeg.toFront()
            billRightLeg2.toFront()
            billRightArm.toFront()
            billRightArm2.toFront()
            billBody.toFront()
            billEyeLeft.toFront()
            billEyeRight.toFront()
            billBowtie.toFront()
            billLeftArm.toFront()
            billLeftArm2.toFront()
            billHat.toFront()
            billHat2.toFront()
            eyelash.toFront()
