    else:
        if(app.oneoclock == True):
            ###Eyeball
            eyeball.visible=True
            eyeballEye.visible=True
            eyeballEye2.visible=True
            eyeballDetail.visible=True
            eyeballDetail2.visible=True
            eyeballDetail3.visible=True
            eyeballDetail4.visible=True
            eyeballDetail5.visible=True
            eyeballDetail6.visible=True
            eyeballDetail7.visible=True
            eyeballDetail8.visible=True
            eyeballDetail9.visible=True
            eyeballDetail10.visible=True
            eyeballDetail11.visible=True
        
        elif(app.twooclock == True):
            ###The ghost woman
            womanHandRight.visible=True
            womanBody.visible=True
            womanHead.visible=True
            womanHair.visible=True
            womanEye1.visible=True
            womanEye2.visible=True
            womanMouth.visible=True
            womanTongue.visible=True
            womanHandLeft.visible=True
    
        elif(app.threeoclock == True):
            ###Bill Cipher :))
            billLeftLeg.visible=True
            billLeftLeg2.visible=True
            billRightLeg.visible=True
            billRightLeg2.visible=True
            billRightArm.visible=True
            billRightArm2.visible=True
            billBody.visible=True
            billEyeLeft.visible=True
            billEyeRight.visible=True
            billBowtie.visible=True
            billLeftArm.visible=True
